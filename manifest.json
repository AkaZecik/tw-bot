{
  "manifest_version": 2,
  "name": "TW-Bot",
  "version": "0.1.0",
  "description": "TW Bot",
  "author": "AkaZecik",
  "icons": {
    "57": "images/tribal_wars_icon.png"
  },
  "background": {
    "scripts": [
      "jquery.min.js",
      "settings.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "TW-Bot",
    "default_icon": {
      "57": "images/tribal_wars_icon.png"
    },
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "permissions": [
    "storage"
  ]
}